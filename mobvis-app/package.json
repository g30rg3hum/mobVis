{
  "name": "mobvis-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next-dev": "next dev",
    "build": "prisma generate && next build",
    "next-start": "next start",
    "start": "concurrently \"npm run py-start\" \"npm run next-start\"",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:py": "pytest",
    "py-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --reload",
    "dev": "concurrently \"npm run next-dev\" \"npm run py-dev\"",
    "postinstall": "prisma generate",
    "py-start": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@hookform/resolvers": "^4.1.0",
    "@prisma/client": "^6.4.0",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slider": "^1.2.4",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@testing-library/user-event": "^14.6.1",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "motion": "^12.6.2",
    "next": "^15.5.4",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.0",
